    "n = int(input())\n",
    "list_=[]\n",
    "\n",
    "for i in range(n):\n",
    "    a,b=input().split()\n",
    "    list_.append([int(a),b])\n",
    "\n",
    "# 나이만으로 소팅하고 굳이 나이가 같다면 이름 소팅은 안하는거라\n",
    "# x[0]으로 설정 \n",
    "# x 예시: [20,\"Dohyun\"]\n",
    "for i in sorted(list_,key=lambda x: x[0]):\n",
    "    print(i[0],i[1])"
   ]
