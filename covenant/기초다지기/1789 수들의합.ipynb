   "source": [
    "import math \n",
    "s =int(input())\n",
    "\n",
    "def add2n(n):\n",
    "    return n*(n+1)/2\n",
    "\n",
    "def findN(s):\n",
    "    start = math.floor(math.sqrt(2*s))\n",
    "    for i in range(start-1,start+1):\n",
    "        if add2n(i)<=s<add2n(i+1):\n",
    "            return i\n",
    "\n",
    "print(findN(s))"
   ]
  }
